import java.util.Scanner;

public class While {

	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
			int x = entrada.nextInt();
			
			int soma = 0;
			while(x!=0) {
				x=entrada.nextInt();
				soma+=x;
			}
		System.out.println(soma);
		entrada.close();
	}
}
