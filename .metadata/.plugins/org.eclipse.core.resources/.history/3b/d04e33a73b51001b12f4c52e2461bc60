import java.util.Scanner;

public class SwitchCase {

	public static void main(FuncoesString[] args) {
		Scanner entrada = new Scanner(System.in);

		int numeroSem;
		FuncoesString dia;

		System.out.println("Digite o número da semana: ");
		numeroSem = entrada.nextInt();

		switch (numeroSem) {
		case 1:
			dia = "Domingo";
			break;
		case 2:
			dia = "Segunda-Feira";
			break;
		case 3:
			dia = "Terça-Feira";
			break;
		case 4:
			dia = "Quarta-Feira";
			break;
		case 5:
			dia = "Quinta-Feira";
			break;
		case 6:
			dia = "Sexta-Feira";
			break;
		case 7:
			dia = "Sábado";
			break;
		default:
			dia = "Dia Semana Inválido";
			break;
		}
		System.out.println(dia);
		entrada.close();
	}
}
