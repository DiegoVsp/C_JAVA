import java.util.Scanner;

public class EntradaDeDados {
	
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
		String x;
		System.out.println("Digite seu primeiro nome: ");
		x=entrada.next();
		
		System.out.println("Você digitou "+x);
		entrada.close();
	}
}
